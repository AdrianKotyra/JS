// Julias_data1
DATA1=[3,5,2,12,7]
// Kates data1
DATA2=[4,1,15,8,3]

newData1 = DATA1.slice(1,-2)
console.log(newData1);

const checkDogs = function(dogsJulia, dogsKate) {
    const Mainlist = [...dogsJulia,...dogsKate]
    Mainlist.forEach(function(age, i) {
        age >=5?  console.log(`This is dog number ${i+1} and its ${age} years old adult dog`) :
        console.log(`This is dog number ${i+1} and its ${age} years old puppy`);;

    })

}
checkDogs(newData1, DATA2 )
